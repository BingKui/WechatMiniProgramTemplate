        },
        "scope.userLocationBackground": {
            "desc": "后台更新位置信息，实时更新"
        },
        "scope.werun": {
            "desc": "读取步数，参与活动"
        },
        "scope.record": {
            "desc": "录制音频分享"
        },
        "scope.writePhotosAlbum": {
            "desc": "保存图片到手机"
        },
        "scope.camera": {
            "desc": "拍摄照片，扫描二维码"
        }
    },
    "sitemapLocation": "sitemap.json",
    "style": "v2",
    "useExtendedLib": {
        "kbone": false,
        "weui": false
    },
    "entranceDeclare": false,
    "darkmode": true,
    "themeLocation": "",
    "lazyCodeLoading": "",
    "singlePage": {
        "navigationBarFit": "squeezed"
