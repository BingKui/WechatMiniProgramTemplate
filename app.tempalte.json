    "networkTimeout": {
        "request": 10000,
        "connectSocket": 10000,
        "uploadFile": 10000,
        "downloadFile": 10000
    },
    "debug": true,
    "functionalPages": false,
    "workers": "",
    "requiredBackgroundModes": ["audio", "location"],
    "plugins": {},
    "subpackages": [{
        "root": "package-common",
        "name": "common",
        "pages": [
            "pages/one"
        ]
    }],
    "preloadRule": {
        "pages/index": {
            "network": "all",
            "packages": ["important"]
